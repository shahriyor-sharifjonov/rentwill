mixin quiz()
    .quiz
        form(action="#!").quiz__body.container 
            h2.quiz__title 1. Где планируете ремонт?
            .quiz__progress     
                .quiz__progress-track
            .quiz__content
                .quiz__item.quiz__1.active
                    .quiz__item-el.w-20 
                        .quiz__item-el-img
                            input(type="radio" name="quiz-1").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-1.png", alt="" draggable="false")
                            .quiz__item-el-i
                        p Новостройка
                    .quiz__item-el.w-20 
                        .quiz__item-el-img
                            input(type="radio" name="quiz-1").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-2.png", alt="" draggable="false")
                            .quiz__item-el-i
                        p Вторичка
                    .quiz__item-el.w-20 
                        .quiz__item-el-img
                            input(type="radio" name="quiz-1").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-3.png", alt="" draggable="false")
                            .quiz__item-el-i
                        p Дом/Коттедж
                    .quiz__item-el.w-20 
                        .quiz__item-el-img
                            input(type="radio" name="quiz-1").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-4.png", alt="" draggable="false")
                            .quiz__item-el-i
                        p Офис
                    .quiz__item-el.w-20 
                        .quiz__item-el-img
                            input(type="radio" name="quiz-1").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-5.png", alt="" draggable="false")
                            .quiz__item-el-i
                        p Другой обьект      
                .quiz__item.quiz__2                    
                    .quiz__item-el.w-20 
                        .quiz__item-el-img
                            input(type="radio" name="quiz-2").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-1.png", alt="" draggable="false")
                            .quiz__item-el-i
