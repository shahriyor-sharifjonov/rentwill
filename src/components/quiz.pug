                    .quiz__item-el.w-20
                        .quiz__item-el-img
                            input(type="radio" name="quiz-6").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-3.png", alt="" draggable="false")
                            .quiz__item-el-i
                        p Через несколько месяцев				
                    .quiz__item-el.w-20
                        .quiz__item-el-img
                            input(type="radio" name="quiz-6").quiz__check
                            .quiz__item-el-border 
                            img(src="@img/quiz-1-4.png", alt="" draggable="false")
                            .quiz__item-el-i
